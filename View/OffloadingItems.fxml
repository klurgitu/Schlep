<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckMenuItem?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="Controllers.OffloadingItemsController">
   <children>
      <Label layoutX="207.0" layoutY="73.0" text="Offloading Items You Already Have" />
      <Button fx:id="nextButton" layoutX="515.0" layoutY="361.0" mnemonicParsing="false" onAction="#saveItemsAndMove" text="Next" />
      <MenuButton fx:id="menuButton" layoutX="230.0" layoutY="113.0" mnemonicParsing="false" text="Select All That Apply">
        <items>
            <CheckMenuItem fx:id="handTruckItem" mnemonicParsing="false" text="Hand Truck" />
            <CheckMenuItem fx:id="rampItem" mnemonicParsing="false" text="Ramp" />
            <CheckMenuItem fx:id="dollyItem" mnemonicParsing="false" text="Dolly" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
